{
  "name": "settings-test",
  "description": "a repo for testing functionality of probot/settings",
  "license": "MIT",
  "repository": "travi-test/settings-test",
  "bugs": "https://github.com/travi-test/settings-test/issues",
  "homepage": "https://github.com/travi-test/settings-test#readme",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null"
  },
  "devDependencies": {
    "ban-sensitive-files": "1.9.17",
    "commitlint-config-travi": "1.4.8",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "lockfile-lint": "4.6.2",
    "npm-run-all": "4.1.5",
    "remark-cli": "10.0.1",
    "remark-preset-lint-travi": "2.0.9",
    "remark-toc": "8.0.1"
  }
}
